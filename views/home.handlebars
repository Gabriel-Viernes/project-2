<header class="LOheader">
    <button id="logoutBtn">Logout</button>
</header>
<section class="home">
    <div class="formcontainer">
        <form class="form" action="http://localhost:3001/api/images" method="post" enctype="multipart/form-data">
            <label class="upload" for="upload">Upload</label>
            <input type="file" id="upload" name="upload">
            <input class="submit" type="submit" value="Submit">
        </form>
    </div>

<section class="homecontainer">
    {{#each images as |image| }}
    <div class="content">
    <span class="imagecontainer">
        <img class='homeimage' src='{{image.filename}}' alt="image">
    </span>
    <div>
        {{image.user.user}}
    </div>
    </div>
    {{/each}}
</section>
<script src="/js/logout.js"></script>
</section>
